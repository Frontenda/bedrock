include icon

mixin iconsOverview(mode)
    .br-sample(id="icons")
        .br-sample-header.br-sample-clearfix
            h3.br-sample-heading
                | Icon list
            a.br-sample-header-link(href="#icons") #
        .br-sample-content
            each icon in icons
                div
                    if mode === 'svg'
                        +icon(icon)
                    if mode === 'icon-font'
                        i.glyphicon(class="glyphicon-#{icon}")
                    code= icon
